generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Register {
  id              Int @id @default(autoincrement())
  username        String @unique
  registrationNo  String @unique
  email           String @unique
  password        String
}

model Login {
  id       Int @id @default(autoincrement())
  username String
  password String
}

model Update {
  id             Int @id @default(autoincrement())
  username       String
  registrationNo String
  email          String
}